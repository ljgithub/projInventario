<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="Inventario_Proj_PU" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>java:app/jn_Inventarios</jta-data-source>
    <class>com.uisrael.inventarioproyecto.modelo.Entidades.Marca</class>
    <class>com.uisrael.inventarioproyecto.modelo.Entidades.Presentacion</class>
    <class>com.uisrael.inventarioproyecto.modelo.Entidades.Producto</class>
    <class>com.uisrael.inventarioproyecto.modelo.Entidades.Proveedor</class>
    <class>com.uisrael.inventarioproyecto.modelo.Entidades.Zona</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
